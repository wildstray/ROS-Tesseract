{
   "ros":{
      "address":"172.16.10.1",
      "user":"admin",
      "password":"admin"
   },
   "boxes":[
      {
         "name":"Routerboard",
         "query":[
            "/system/routerboard/print"
         ],
         "column":0,
         "color":1
      },
      {
         "name":"Clock",
         "query":[
            "/system/clock/print"
         ],
         "column":0,
         "color":1
      },
      {
         "name":"Resources",
         "query":[
            "/system/resource/monitor",
            "=once"
         ],
         "column":0,
         "color":1
      },
      {
         "name":"LTE",
         "query":[
            "/interface/lte/info",
            "=number=0",
            "=duration=0.1",
            "=once"
         ],
         "column":1,
         "padding":1,
         "color":1,
         "disabled":false
      },
      {
         "name":"RSSI",
         "query":[
            "/interface/lte/info",
            "=number=0",
            "=duration=0.1",
            "=once",
            "=.proplist=rssi"
         ],
         "column":1,
         "color":1,
         "gauge":true,
         "min":-110,
         "max":-45,
         "units":"dBm",
         "disabled":false
      },
      {
         "name":"RSRP",
         "query":[
            "/interface/lte/info",
            "=number=0",
            "=duration=0.1",
            "=once",
            "=.proplist=rsrp"
         ],
         "column":1,
         "color":1,
         "graph":true,
         "min":-120,
         "max":-60,
         "units":"seconds",
         "interval":60,
         "height":24,
         "type": "bars",
         "disabled":false
      },
      {
         "name":"Cell monitor",
         "query":[
            "/interface/lte/cell-monitor",
            "=number=0",
            "=duration=0.1",
            "=freeze-frame-interval=0.1"
         ],
         "column":1,
         "height":20,
         "color":1,
         "disabled":true
      },
      {
         "name":"IP address",
         "query":[
            "/ip/address/print",
            "=.proplist=address,network,interface,dynamic,disabled",
            "?disabled=no"
         ],
         "column":2,
         "padding":1,
         "color":1
      },
      {
         "name":"Routing",
         "query":[
            "/ip/route/print",
            "=.proplist=dst-address,gateway,distance,active,dynamic,static"
         ],
         "column":2,
         "padding":2,
         "color":1,
         "disabled":false
      },
      {
         "name":"Traceroute",
         "query":[
            "/tool/traceroute",
            "=address=8.8.8.8",
            "=protocol=icmp",
            "=max-hops=15",
            "=count=1",
            "=timeout=0.1"
         ],
         "column":2,
         "height":20,
         "color":1
      }
   ],
   "colorpairs":[
      {
         "pair":1,
         "fg":"COLOR_WHITE",
         "bg":"COLOR_BLUE"
      },
      {
         "pair":2,
         "fg":"COLOR_GREEN",
         "bg":"COLOR_BLACK"
      }
   ],
   "color":1
}
